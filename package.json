{
  "name": "industry-portal",
  "version": "0.0.4",
  "description": "Mujo industry portal",
  "main": "index.js",
  "scripts": {
    "start": "brunch watch --server",
    "build": "brunch build --production",
    "postinstall": "npm run build",
    "watch": "brunch watch",
    "predebug": "echo In one window launch brunch without server: npm run watch",
    "debug": "echo Then in second window launch server with debug: node-debug server.js",
    "test": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive -R spec",
    "test-0.10": "node --harmony node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec",
    "predeploy": "cd dist && git pull origin gh-pages",
    "deploy": "npm run build",
    "postdeploy": "cd dist && git add -A && git commit -m \"Latest version\" && git push origin gh-pages",
    "postversion": "git push origin master && git push --tags",
    "patch": "npm version patch -m \"Upgrade to v%s\"",
    "minor": "npm version minor -m \"Upgrade to v%s\"",
    "major": "npm version major -m \"Upgrade to v%s\"",
    "docker": "docker-compose up -d && node db/recreate_database.js",
    "docker:stop": "docker-compose down",
    "docker:bash": "docker exec -it industryportal_mysql_1 bash"
  },
  "author": "Richard Williams",
  "license": "ISC",
  "devDependencies": {
    "async": "^2.0.1",
    "auto-reload-brunch": "^2.7.1",
    "brunch": "^2.8.2",
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "jade-brunch": "^2.8.0",
    "javascript-brunch": "^2.0.0",
    "mocha": "^3.0.2",
    "sass-brunch": "^2.6.3"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "chart.js": "^2.2.1",
    "connect-flash": "^0.1.1",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "jade": "^1.11.0",
    "localforage": "^1.4.3",
    "mongoose": "^4.6.1",
    "morgan": "^1.7.0",
    "mysql": "^2.11.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "serve-favicon": "^2.3.0"
  },
  "engines": {
    "node": "4.2.4"
  }
}
