{
  "name": "industry-portal",
  "version": "0.0.4",
  "description": "Mujo industry portal",
  "main": "index.js",
  "scripts": {
    "start": "brunch watch --server",
    "prebuild": "ncp ./node_modules/bootstrap/fonts/ ./app/assets/fonts/",
    "build": "brunch build --production",
    "postinstall": "npm run build",
    "watch": "brunch watch",
    "debug": "node --inspect --debug-brk server.js",
    "test": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive -R spec",
    "test-0.10": "node --harmony node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec",
    "postversion": "git push origin master && git push --tags",
    "patch": "npm version patch -m \"Upgrade to v%s\"",
    "minor": "npm version minor -m \"Upgrade to v%s\"",
    "major": "npm version major -m \"Upgrade to v%s\"",
    "docker": "docker-compose up -d && node db/recreate_database.js",
    "docker:stop": "docker-compose down",
    "docker:bash": "docker exec -it industryportal_mysql_1 bash"
  },
  "author": "Richard Williams",
  "license": "ISC",
  "devDependencies": {
    "async": "^2.0.1",
    "auto-reload-brunch": "^2.7.1",
    "babel-eslint": "^7.2.1",
    "brunch": "^2.8.2",
    "chai": "^3.5.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.3",
    "istanbul": "^0.4.5",
    "jade-brunch": "^2.8.0",
    "javascript-brunch": "^2.0.0",
    "mocha": "^3.0.2",
    "sass-brunch": "^2.6.3"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "bootstrap-select": "^1.12.2",
    "chart.js": "^2.2.1",
    "compression": "^1.6.2",
    "connect-flash": "^0.1.1",
    "csv-stringify": "^1.0.4",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "jade": "^1.11.0",
    "jquery": "^1.12.4",
    "localforage": "^1.4.3",
    "metismenu": "^2.7.0",
    "mongoose": "^4.6.1",
    "morgan": "^1.7.0",
    "mysql": "^2.11.1",
    "ncp": "^2.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "redbird": "^0.6.5",
    "serve-favicon": "^2.3.0",
    "simple-statistics": "^2.5.0"
  },
  "engines": {
    "node": "4.2.4"
  }
}
